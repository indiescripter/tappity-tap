{
  "name": "test-anywhere",
  "version": "1.0.0",
  "metapackage": "typeverse-node-app",
  "description": "A wrapper package for node-tap with TypeScript typings",
  "keywords": [
    "node-tap",
    "tap",
    "test",
    "testing"
  ],
  "copyright": "Copyright (c) 2020 Justin Johansson",
  "license": "UNLICENSED",
  "repository": "https://gitlab.com/indiescripter/test-anywhere.git",
